<div id="myCarousel1" class="carousel slide" data-ride="carousel1" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel1" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel1" data-slide-to="1"></li>

    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="Recursos/Agentes/DUELISTA (2).png" usemap="#duelistas" alt="Funções" class="img-responsive" height="360" width="1440" />
